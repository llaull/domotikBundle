{% extends 'BackOfficeBundle:Metronic:body-page-content-managed_datatables.html.twig' %}

{% set title_page = 'historiques modules' %}
{% set path_entity_new = 'd' %}

{% block content_txt %}
<tr>
    <th>Created</th>
    <th>Modified</th>
    <th>Id</th>
    <th>Name</th>
    <th>Adressmac</th>
    <th>Adressipv4</th>
    <th>Actions</th>
</tr>
</thead>
<tbody>
{% for entity in entities %}
    <tr class="odd gradeX">
        <td>{{ entity.created|date('Y-m-d H:i:s') }}</td>
        <td>{% if entity.modified %}{{ entity.modified|date('Y-m-d H:i:s') }}{% endif %}</td>
        <td>{{ entity.id }}</td>
        <td>{{ entity.name }}</td>
        <td>{{ entity.adressMac }}</td>
        <td>{{ entity.adressIpv4 }}</td>
        <td>
            <ul>
                <li>
                    <a href="{{ path('admin_domotique_modules_show', { 'id': entity.id }) }}">show</a>
                </li>
                <li>
                    <a href="{{ path('admin_domotique_modules_edit', { 'id': entity.id }) }}">edit</a>
                </li>
            </ul>
        </td>
    </tr>
{% endfor %}
{% endblock content_txt %}
